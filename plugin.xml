<?xml version="1.0" encoding="UTF-8"?>
<plugin xmlns="http://apache.org/cordova/ns/plugins/1.0"
        id="cordova-plugin-brother-bluetooth-print" version="1.0.0">
    <name>Brother Bluetooth Print</name>
    <description></description>
    <license>Apache 2.0</license>
    <keywords>cordova,brother,bluetooth,print</keywords>
    <js-module src="www/plugin.js" name="plugin">
        <runs/>
        <clobbers target="BrotherBluetoothPrint" />
    </js-module>
    <platform name="android">
        <config-file target="config.xml" parent="/*">
            <feature name="BrotherBluetoothPrint">
                <param name="android-package" value="com.example.BrotherBluetoothPrint"/>
                <param name="onload" value="true" />
            </feature>
        </config-file>

        <source-file src="src/android/com/example/BrotherBluetoothPrint.java" target-dir="src/com/example" />
    </platform>
</plugin>
